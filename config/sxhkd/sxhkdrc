XF86Audio{Lower,Raise}Volume
	amixer -q sset Master 3%{-,+} unmute && notify_sound_level

super + {Down,Up}
	amixer -q sset Master 3%{-,+} unmute && notify_sound_level 

XF86MonBrightness{Down,Up}
	xbacklight -{dec,inc} 5 && notify_brightness_level

alt + shift + p
    $HOME/.config/polybar/launch.sh

alt + p
	passmenu

alt + d
    rofi_launch &

alt + shift + d
    rofi_dnd &

alt + shift + x
    rofi_shutdown &

super + w
    wifi_setup &

Print
    take_screenshot fullscreen

F12
    take_screenshot

alt + shift + w
	$BROWSER

alt + s
    echo cmd:toggle > /tmp/polybar_mqueue.*

