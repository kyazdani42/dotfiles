XF86Audio{Lower,Raise}Volume
	amixer -q sset Master 3%{-,+} unmute

super + {Down,Up}
	amixer -q sset Master 3%{-,+} unmute

XF86MonBrightness{Down,Up}
	xbacklight -{dec,inc} 5

alt + shift + o
	dm-tool switch-to-greeter

alt + shift + p
    $HOME/.config/polybar/launch.sh > /dev/null

alt + p
	passmenu

alt + c
    rofi_dnd

alt + d
    rofi_launch

alt + shift + x
    shutdown_dialog

Print
    scrot -q 100

F12
    scrot -q 100 -s -l style=solid,width=2 -d 2

alt + shift + w
	$BROWSER

alt + s
    echo cmd:toggle > /tmp/polybar_mqueue.*

