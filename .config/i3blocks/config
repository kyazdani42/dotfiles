separator_block_width=20
separator=false
align=center

[ips]
command=$HOME/.scripts/ips
markup=pango
interval=once

[memory swap cpu usage]
command=$HOME/.scripts/usageinfo
interval=3

[battery]
command=$HOME/.scripts/battery
markup=pango
interval=3

[datetime]
command=echo $(date +"%F %T")
interval=1

